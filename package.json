{
  "name": "appwrite-backup-tool",
  "version": "1.0.0",
  "description": "AppWrite backup toolset",
  "scripts": {
    "appwrite:backup": "node ./scripts/backup.mjs",
    "appwrite:restore": "node ./scripts/restore.mjs",
    "appwrite:diff": "node ./scripts/diff.mjs",
    "appwrite:create-users": "node ./scripts/create-users.mjs",
    "appwrite:create-collection": "node ./scripts/create-collection.mjs",
    "appwrite:fetch": "sh ./tools/appwrite_fetch.sh",
    "appwrite:push": "sh ./tools/appwrite_push.sh",
    "appwrite:fetch:windows": ".\\tools\\appwrite_fetch.bat",
    "appwrite:push:windows": ".\\tools\\appwrite_push.bat",
    "appwrite:start": "cd docker && sh ./run.sh",
    "appwrite:start:windows": "cd docker & run.bat",
    "unsafe:remove-backup": "npx -y rimraf backup ./backup",
    "unsafe:uninstall-appwrite": "sh ./tools/docker_cleanup.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "16.3.1",
    "glob": "10.3.4",
    "node-appwrite": "10.0.0"
  },
  "devDependencies": {
    "@types/glob": "8.1.0"
  }
}
